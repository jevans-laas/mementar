<launch>
    <arg name="intern_directory" default="$(find mementar)/file_intern"/>
    <arg name="config_file" default="none"/>

    <node pkg="mementar" name="ontologenius_core" exec="mementar_multi" output="screen" args="-d $(arg intern_directory) -c $(arg config_file)"> </node>
    <node pkg="mementar" name="ontologenius_gui" exec="mementarGUI" output="screen"> </node>
</launch>